{
  "entrypoint": "index.html",
  "shell": "src/quote-center-app/quote-center-app.js",
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "sources": [
    "src/**/*",
    "assets/**/*",
    "data/**/*",
    "bower.json",
    "public/assets/**/*",
    "manifest.json"
  ],
  "extraDependencies": [
    "node_modules/@polymer/app-storage/app-indexeddb-mirror/app-indexeddb-mirror-worker.js",
    "node_modules/@polymer/app-storage/app-indexeddb-mirror/common-worker-scope.js",
    "node_modules/@webcomponentsjs/**",
    "manifest.json"
  ],
  "builds": [{
    "bundle": true,
    "js": {"minify": true, "compile": true},
    "css": {"minify": true},
    "html": {"minify": true},
    "addServiceWorker": true,
    "swPrecacheConfig": "./sw-precache-config.js"
  }]
}
