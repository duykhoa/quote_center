{
  "entrypoint": "index.html",
  "shell": "src/quote-center-app/quote-center-app.js",
  "lint": {
    "rules": [
      "polymer-3"
    ],
    "ignoreWarnings": ["not-loadable"]
  },
  "sources": [
    "src/**/*",
    "assets/**/*",
    "public/assets/**/*",
    "manifest.json"
  ],
  "extraDependencies": [
    "node_modules/@webcomponents/**",
    "manifest.json"
  ],
  "builds": [{
    "bundle": true,
    "js": {"minify": true, "compile": true},
    "css": {"minify": true},
    "html": {"minify": true},
    "addServiceWorker": true,
    "swPrecacheConfig": "./sw-precache-config.js"
  }]
}
